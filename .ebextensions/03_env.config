option_settings:
  aws:elasticbeanstalk:application:environment:
    SECRET_KEY: 'super-secret'
    POETRY_HOME: '/usr/local/bin/poetry'
    JWT_SECRET_KEY: 'super-secret'

    DB_HOST:
      "Fn::GetAtt": ["AWSEBRDSDatabase", "Endpoint.Address"]
